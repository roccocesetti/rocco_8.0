                   			<field name="usr_nods"/>
                   			<field name="passwd_nods"/>
                    		<field name="url_nods"/>
                        </group>

                        <group>
                            <field name="ip_pubblico" />
                            <field name="ip_locale"/>
                   			<field name="id_vm"/>
                        </group>
                        <group>
                            <field name="applicazione"/>
                            <field name="x_service_partner" string="Cliente associato"/>
                        </group>
                    </group>
                    </sheet>
                </form>
            </field>
        </record>

