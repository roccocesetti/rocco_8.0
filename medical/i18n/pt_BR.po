#. module: medical
#: model:email.template,body_html:medical.email_template_appointment_confirmation
msgid ""
"\n"
"                <div class=\"content-wrapper\" style=\"max-width:650px;\">\n"
"                    <div class=\"col-sm-12 highlight\">\n"
"                        <p> <b>Dear ${object.patient_id.name}</b> </p>\n"
"                        <p>The following appointment has been created successfully</p>\n"
"                        <div class=\"row\"></div>\n"
"\n"
"                        <div class=\"col-sm-8 highlight\">\n"
"                            <p>Doctor....: <b>${object.physician_id.name}</b></p>\n"
"                            <p>Date.....: <b>${ctx.get('appointment_date') or object.appointment_date[:10]}</b><img class=\"img-responsive\" style=\"float:right;\" src=\"http://agendese.com/website/static/src/img/Agendese/Banners/calendar.png\"></p>\n"
"                            <p>Time......: <b>${ctx.get('appointment_time') or object.appointment_date[11:]}</b></p>\n"
"                            <p>Clinic...: <b>${object.institution_id.name}</b></p>\n"
"                            <p>Phone..: <b>${object.institution_id.phone}</b></p>\n"
"                            <p>Address.: <b>${object.institution_id.street} ${object.institution_id.street2}</b></p>\n"
"                        </div>\n"
"                    </div>\n"
"                </div>\n"
"            "
msgstr ""
"\n"
"                <div class=\"content-wrapper\" style=\"max-width:650px;\">\n"
"                    <div class=\"col-sm-12 highlight\">\n"
"                        <p> <b>Prezado ${object.patient_id.name}</b> </p>\n"
"                        <p>Os seguintes agendamentos foram criados com sucesso</p>\n"
"                        <div class=\"row\"></div>\n"
"\n"
"                        <div class=\"col-sm-8 highlight\">\n"
"                            <p>Doutor....: <b>${object.physician_id.name}</b></p>\n"
"                            <p>Data......: <b>${ctx.get('appointment_date') or object.appointment_date[:10]}</b><img class=\"img-responsive\" style=\"float:right;\" src=\"http://agendese.com/website/static/src/img/Agendese/Banners/calendar.png\"></p>\n"
"                            <p>Hora......: <b>${ctx.get('appointment_time') or object.appointment_date[11:]}</b></p>\n"
"                            <p>Clínica...: <b>${object.institution_id.name}</b></p>\n"
"                            <p>Telefone..: <b>${object.institution_id.phone}</b></p>\n"
"                            <p>Endereço..: <b>${object.institution_id.street} ${object.institution_id.street2}</b></p>\n"
"                        </div>\n"
"                    </div>\n"
"                </div>\n"
"            "

